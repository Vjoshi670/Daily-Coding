class Solution {
    public int countDigitOne(int n) {
        if (n == 0) return 0;

        int count = 0;
        int a = 0, b = 1;

        while (n / b > 0) {
            int m = (n / b) % 10;
            if (m > 0) count += m * a + Math.min(b, n % (b * 10) - (b - 1));

            a = 10 * a + b;
            b *= 10;
        }
        return count;
    }
}
